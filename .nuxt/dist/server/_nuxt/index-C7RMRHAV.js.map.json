{"file":"index-C7RMRHAV.js","mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAIA,UAAM,QAAQ,IAAI,EAAE;AACpB,UAAM,UAAU,IAAI,IAAI;AACxB,UAAM,QAAQ,IAAI,IAAI;;;;;;;;;;;;;;;;;;;;;;;;;;;;","names":[],"sources":["../../../../pages/teams/index.vue"],"sourcesContent":["<script setup>\nimport { useHockeyApi } from '~/composables/useApi';\n\nconst { fetchTeams } = useHockeyApi();\nconst teams = ref([]);\nconst loading = ref(true);\nconst error = ref(null);\n\nonMounted(async () => {\n  try {\n    const response = await fetchTeams();\n    teams.value = response.data;\n  } catch (err) {\n    console.error('Failed to fetch teams:', err);\n    error.value = 'Failed to fetch teams.';\n  } finally {\n    loading.value = false;\n  }\n});\n</script>\n\n<template>\n  <main>\n    <section class=\"container mx-auto\">\n      <h2 class=\"text-2xl mt-3 mb-4 text-center\">All Teams</h2>\n      <div v-if=\"loading\" class=\"text-center\">Loading...</div>\n      <div v-else-if=\"error\" class=\"text-center text-zinc-700\">{{ error }}</div>\n      <div v-else class=\"grid grid-cols-1 gap-4 md:grid-cols-2 lg:grid-cols-3\">\n        <TeamCard v-for=\"team in teams\" :team=\"team\" :key=\"team.id\" />\n      </div>\n    </section>\n  </main>\n</template>\n"],"version":3}